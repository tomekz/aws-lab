{
    "family": "service", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "hello-service", 
            "image": "925303156481.dkr.ecr.eu-central-1.amazonaws.com/lab-kafka-ecr:v0.0.1", 
            "portMappings": [
                {
                    "containerPort": 3000, 
                    "hostPort": 3000, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512",
    "execution_role_arn": "arn:aws:iam::925303156481:role/lab-kafka-ecs-service-execution-task-role"
}
