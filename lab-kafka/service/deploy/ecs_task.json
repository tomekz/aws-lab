{
    "family": "service", 
    "networkMode": "awsvpc", 
    "containerDefinitions": [
        {
            "name": "hello-service", 
            "image": "925303156481.dkr.ecr.eu-central-1.amazonaws.com/lab-kafka-ecr:latest", 
            "portMappings": [
                {
                    "containerPort": 80, 
                    "hostPort": 80, 
                    "protocol": "tcp"
                }
            ], 
            "essential": true
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512"
}
